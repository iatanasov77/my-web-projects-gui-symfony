<div>
	{{ form_start(form, { 'attr': {'class': 'form-horizontal ', 'id': 'formProjectInstallManual'} }) }}
		<div class="formFields">
			<div class="form-group">
				{{ form_label(form.installManual, null, {
				    'label_attr': {'class': 'col-sm-8 control-label'}
				    }) 
				}}
		
				{{ form_widget(form.installManual, {'attr': {'class': 'form-control'}}) }}
			</div>
		</div>
		
		<div class="form-group formButtons">
			{{ form_widget(form.btnSave, {'attr': {'class': 'btn btn-primary', 'id':'btnSave'}}) }}
		</div>
    {{ form_end(form, {'render_rest': false}) }}
</div>
