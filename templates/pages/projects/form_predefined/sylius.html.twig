<div class="form-group">
    <label>Sylius Project Variants: </label>
    <select class="form-control select-button phpVersionFormInput" name="predefinedTypeParams[variant]">
        {% for key, name in variants %}
            <option value="{{ key }}">{{ name }}</option>
        {% endfor %}
    </select>
</div>

<div class="form-group">
    <label>Sylius Version: </label>
    <select class="form-control select-button phpVersionFormInput" name="predefinedTypeParams[version]">
        {% for key, name in branches %}
            <option value="{{ key }}">{{ name }}</option>
        {% endfor %}
        {#
        <option value="1.7">1.7</option>
    	<option value="1.6">1.6</option>
    	<option value="1.5">1.5</option>
    	<option value="1.8">1.8</option>
    	#}
    </select>
</div>

<div class="form-group">
    <input type="checkbox" name="predefinedTypeParams[installSampleData]" />
    <label>Install sample data</label>
</div>
